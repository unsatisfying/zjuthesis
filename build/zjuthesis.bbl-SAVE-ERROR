    \entry{returning_exploits2025}{inproceedings}{}
      \name{author}{3}{}{%
        {{hash=fd6408c53bf54b1e6ae7b7df03f1e1e3}{%
           family={Roels},
           familyi={R\bibinitperiod},
           given={Jonas},
           giveni={J\bibinitperiod}}}%
        {{hash=b2786dcb13c252ed66008b80b7a2b4a5}{%
           family={Jacobs},
           familyi={J\bibinitperiod},
           given={Adriaan},
           giveni={A\bibinitperiod}}}%
        {{hash=4332b969e8cfdf03e5c2b389e41ca7bc}{%
           family={Volckaert},
           familyi={V\bibinitperiod},
           given={Stijn},
           giveni={S\bibinitperiod}}}%
      }
      \name{namea}{3}{}{%
        {{hash=fd6408c53bf54b1e6ae7b7df03f1e1e3}{%
           family={Roels},
           familyi={R\bibinitperiod},
           given={Jonas},
           giveni={J\bibinitperiod}}}%
        {{hash=b2786dcb13c252ed66008b80b7a2b4a5}{%
           family={Jacobs},
